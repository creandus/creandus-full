compat-GNU-glibc-useradd() {
	local home="${1}" groups="${2}" shell="${3}"
	local uid="${4}" comment="${5}" name="${6}"

	useradd -d "${userhome}" -G "${usergroups}" \
		-s "${usershell}" -u "${userid}" \
		-c "${usercomment}" "${newuser}"
}
